{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm exec vite",
    "build": "pnpm exec vue-tsc -b && pnpm exec vite build",
    "build:analyze": "pnpm exec vue-tsc -b && pnpm exec vite build --mode analyze",
    "build:prod": "pnpm exec vue-tsc -b && pnpm exec vite build --mode production",
    "preview": "pnpm exec vite preview",
    "preview:dist": "pnpm exec vite preview --port 4173 --host",
    "lint": "pnpm exec eslint --fix --ext .vue,.ts .",
    "bundle-analyzer": "pnpm exec vite-bundle-analyzer dist"
  },
  "dependencies": {
    "@internationalized/date": "^3.10.0",
    "@tailwindcss/vite": "^4.1.13",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "decimal.js": "^10.6.0",
    "echarts": "^5.6.0",
    "embla-carousel-vue": "^8.6.0",
    "lightweight-charts": "^4.2.3",
    "lodash-es": "^4.17.21",
    "lucide-vue-next": "^0.544.0",
    "pinia": "^3.0.3",
    "reka-ui": "^2.5.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "vaul-vue": "^0.4.1",
    "vue": "^3.5.21",
    "vue-echarts": "^7.0.3",
    "vue-i18n": "^11.1.12",
    "vue-sonner": "^2.0.9"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.5.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.5.0",
    "globals": "^16.4.0",
    "terser": "^5.44.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.7",
    "vue-tsc": "^3.0.7"
  }
}
